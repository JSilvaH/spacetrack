<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.silvacomp.spacetrack">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".SpaceTrack"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_spacetrack_launch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_spacetrack_launch_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp.Starting"
        tools:targetApi="31">
        <activity
            android:name=".launchpads.presentation.LaunchpadActivity"
            android:exported="false"
            android:label="@string/title_activity_launchpad"
            android:theme="@style/Theme.App.Starting" />
        <activity
            android:name=".dragons.presentation.DragonActivity"
            android:exported="false"
            android:label="@string/title_activity_dragon"
            android:theme="@style/Theme.App.Starting" />
        <activity
            android:name=".crew.presentation.CrewActivity"
            android:exported="false"
            android:label="@string/title_activity_crew"
            android:theme="@style/Theme.App.Starting" />
        <activity
            android:name=".rockets.presentation.RocketScreenDetail"
            android:exported="false"
            android:label="@string/title_activity_rocket_screen_detail"
            android:theme="@style/Theme.App.Starting" />

        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>